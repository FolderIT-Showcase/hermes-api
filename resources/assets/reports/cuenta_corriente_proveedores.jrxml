	<queryString language="SQL">
		<![CDATA[SELECT cc.fecha,
	tcc.nombre, 
	com.numero, 
	cc.debe, 
	cc.haber, 
	pro.nombre as nombre_proveedor, 
	cc.updated_at
FROM cta_cte_proveedores cc
	LEFT JOIN comprobantes_compras com ON com.id = cc.comprobante_compras_id
	LEFT JOIN ordenes_pago op ON op.id = cc.orden_pago_id,
	proveedores pro,
	tipo_comprobantes_compras tcc
WHERE
	pro.id = $P!{PROVEEDOR_ID}
	AND cc.proveedor_id = pro.id
	AND tcc.id = cc.tipo_comp_compras_id
	AND cc.fecha BETWEEN $P{FECHA_INICIO} AND $P{FECHA_FIN}
ORDER BY  cc.fecha, cc.updated_at ASC]]>
	</queryString>
	<field name="fecha" class="java.sql.Date"/>
	<field name="nombre" class="java.lang.String"/>
	<field name="numero" class="java.math.BigDecimal"/>
	<field name="debe" class="java.math.BigDecimal"/>
	<field name="haber" class="java.math.BigDecimal"/>
	<field name="nombre_proveedor" class="java.lang.String"/>
	<variable name="saldo" class="java.lang.Double">
		<variableExpression><![CDATA[SUM($V{saldo},$F{debe},PRODUCT(-1,$F{haber}))]]></variableExpression>
		<initialValueExpression><![CDATA[new Double("0")]]></initialValueExpression>
	</variable>
