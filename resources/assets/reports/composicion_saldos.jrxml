	<queryString language="SQL">
		<![CDATA[SELECT com.fecha, com.punto_venta, com.numero, tc.codigo,
			com.importe_total, c.nombre as nombre_cliente,
			c.codigo as codigo_cliente, com.saldo
FROM clientes c,
	comprobantes com,
	tipo_comprobantes tc
WHERE
	 com.cliente_id = c.id
	 $P!{whereCliente}
	 $P!{whereVendedor}
	 $P!{whereZona}
	 AND tc.id = com.tipo_comprobante_id
	 AND tc.codigo NOT LIKE 'PR_'
	 AND saldo <> 0
ORDER BY c.nombre, com.fecha, com.updated_at ASC]]>
	</queryString>
