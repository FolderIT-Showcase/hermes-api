	<queryString>
		<![CDATA[SELECT c.fecha, c.numero, c.punto_venta, c.cliente_nombre, c.importe_total, tc.codigo, tc.nombre, l.cp
FROM comprobantes c, tipo_comprobantes tc, clientes cl, domicilios d, localidades l
WHERE c.tipo_comprobante_id = tc.id
AND c.cliente_id = cl.id
AND d.cliente_id = cl.id
AND d.localidad_id = l.id
AND tc.codigo IN $P!{CODIGOS}
AND c.fecha BETWEEN $P{FECHA_INICIO} AND $P{FECHA_FIN}
ORDER BY  c.fecha, tc.codigo, c.updated_at ASC]]>
	</queryString>
