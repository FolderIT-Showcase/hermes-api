	<queryString>
		<![CDATA[SELECT c.fecha, c.numero, c.punto_venta, c.importe_total, tc.codigo, tc.nombre
FROM comprobantes c, tipo_comprobantes tc
WHERE c.tipo_comprobante_id = tc.id
AND tc.codigo IN $P!{CODIGOS}
AND c.fecha BETWEEN $P{FECHA_INICIO} AND $P{FECHA_FIN}
ORDER BY  tc.codigo, c.fecha, c.updated_at ASC]]>
	</queryString>
