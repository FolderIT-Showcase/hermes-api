	<queryString language="SQL">
		<![CDATA[SELECT c.codigo, c.nombre, c.tipo_responsable, c.cuit, c.telefono, d.direccion, l.nombre as localidad_nombre
FROM clientes c, domicilios d, localidades l
WHERE	d.cliente_id = c.id AND
				l.id =d.localidad_id AND 
				d.localidad_id = l.id 
				$P!{whereVendedor}
				$P!{whereZona}
				$P!{whereProvincia}
				$P!{whereLocalidad}
				$P!{whereActivos}
ORDER BY c.nombre ASC]]>
	</queryString>
