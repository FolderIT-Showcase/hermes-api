	<queryString>
		<![CDATA[SELECT c.fecha, c.cliente_tipo_resp, c.numero, c.punto_venta, c.cliente_nombre, c.cliente_cuit, c.alicuota_iva, c.importe_iva, c.importe_neto, c.importe_total, tc.codigo
FROM comprobantes c, tipo_comprobantes tc
WHERE c.tipo_comprobante_id = tc.id
AND tc.codigo NOT LIKE 'PR_'
AND MONTH(c.fecha) = $P{PERIODO_MES}
AND YEAR(c.fecha) = $P{PERIODO_ANIO}
ORDER BY  c.fecha, c.updated_at ASC]]>
	</queryString>
