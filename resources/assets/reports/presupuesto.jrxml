	<queryString language="SQL">
		<![CDATA[SELECT ci.cantidad, ci.importe_unitario, ci.importe_total,
			 c.importe_iva, c.importe_neto, c.importe_total as c_importe_total, 
			 a.codigo, a.nombre, c.fecha, c.numero, c.cliente_nombre
FROM compro_items ci,
	articulos a,
	comprobantes c
WHERE 
	 c.id = $P!{COMPROBANTE_ID} 
	 AND c.id = ci.comprobante_id 
	 AND ci.articulo_id = a.id
]]>
