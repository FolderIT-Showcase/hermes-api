	<queryString>
		<![CDATA[SELECT 
	periodo_id,
	fecha,
	codigo AS tcc_codigo,
	proveedor_nombre,
	punto_venta,
	numero,
	proveedor_tipo_resp,
	proveedor_cuit,
	importe_total,
	alicuota_iva,
	importe_neto_gravado,
	importe_neto_no_gravado,
	importe_iva,
	alicuota AS alicuota_ccr,
	importe AS importe_ccr
FROM comprobantes_compras cc,
	comprobantes_compras_importes cci,
	comprobantes_compras_retenciones ccr,
	periodos_fiscales per,
	tipo_comprobantes_compras tcc
WHERE
	periodo_id = $P!{PERIODO_ID} 
	AND tcc.id = cc.tipo_comp_compras_id
    AND per.id = cc.periodo_id
    AND cci.comprobante_id = cc.id
    AND ccr.comprobante_id = cc.id
ORDER BY fecha ASC]]>
	</queryString>
