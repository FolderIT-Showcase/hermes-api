		<parameter name="COBRO_ID" class="java.lang.String">
			<parameterDescription><![CDATA[]]></parameterDescription>
			<defaultValueExpression><![CDATA[37]]></defaultValueExpression>
		</parameter>
		<queryString language="SQL">
			<![CDATA[SELECT tc.codigo as tipo, comp.numero, comp.fecha, comp.punto_venta,
		 	comp.importe_total as total_comprobante,
			comp.saldo, ci.importe_total as importe_pagado, anticipo
FROM cobros c,
	cobro_items ci
	LEFT JOIN comprobantes comp ON comp.id = ci.comprobante_id
	LEFT JOIN tipo_comprobantes tc ON tc.id = comp.tipo_comprobante_id
WHERE 
	 c.id = $P!{COBRO_ID} 
	AND c.id = ci.cobro_id]]>
		</queryString>
		<field name="tipo" class="java.lang.String"/>
		<field name="numero" class="java.math.BigDecimal"/>
		<field name="fecha" class="java.sql.Date"/>
		<field name="punto_venta" class="java.math.BigDecimal"/>
		<field name="total_comprobante" class="java.math.BigDecimal"/>
		<field name="saldo" class="java.math.BigDecimal"/>
		<field name="importe_pagado" class="java.math.BigDecimal"/>
		<field name="anticipo" class="java.lang.Boolean"/>
		<variable name="sum" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{importe_pagado}]]></variableExpression>
			<initialValueExpression><![CDATA[0.00]]></initialValueExpression>
		</variable>
	</subDataset>
