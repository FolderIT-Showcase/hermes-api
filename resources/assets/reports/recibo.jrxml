	<queryString language="SQL">
		<![CDATA[SELECT c.fecha, cv.importe, mp.nombre as mediopago,
	 mp.nombre, b.nombre as b_nombre, tt.nombre as tt_nombre,
	 ct.numero as ct_numero, d.numero as d_numero,
	 ct.importe as ct_importe, t.importe as t_importe,
	 d.importe as d_importe, ct.fecha_vencimiento
FROM cobros c,
	cobro_valores cv
	LEFT JOIN tarjetas t ON t.cobro_valor_id = cv.id
	LEFT JOIN cheques_terceros ct ON ct.cobro_valor_id = cv.id
	LEFT JOIN depositos d ON d.cobro_valor_id = cv.id
	LEFT JOIN cuentas_bancarias cb ON cb.id = d.cuenta_id
	LEFT JOIN bancos b ON (ct.banco_id =b.id OR cb.banco_id = b.id)
	LEFT JOIN tipo_tarjetas tt ON tt.id = t.tarjeta_id,
	medios_pago mp
WHERE 
	 c.id = $P!{COBRO_ID} 
	AND c.id = cv.cobro_id 
	AND cv.medios_pago_id = mp.id]]>
	</queryString>
